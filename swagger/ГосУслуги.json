  "components": {
    "schemas": {
      "User": {
        "type": "object",
        "required": [
          "username",
          "FIO"
        ],
        "properties": {
          "id": {
            "type": "integer",
            "example": "1"
          },
          "id_application": {
            "type": "integer",
            "example": "1"
          },
          "username": {
            "type": "string",
            "example": "krasava"
          },
          "FIO": {
            "type": "string",
            "example": "Сергеев Сегрей Сергеевич"
          },
          "manager": {
            "type": "boolean",
            "example": 0
          },
          "passport": {
            "type": "integer",
            "example": "0316352475"
          },
          "SNILS": {
            "type": "integer",
            "example": "3583295393472"
          },
          "INN": {
            "type": "integer",
            "example": "62345234523"
          }
        }
      },
      "Notifications": {
        "type": "object",
        "required": [
          "name",
          "opisanie"
        ],
        "properties": {
          "id": {
            "type": "integer",
            "example": "1"
          },
          "id_application": {
            "type": "integer",
            "example": "1"
          },
          "name": {
            "type": "string",
            "example": "Заявка на услугу Ипотека на пол века"
          },
          "opisanie": {
            "type": "string",
            "example": "Завявка ипотеки на 50 лет одобрена"
          },
          "Date": {
            "type": "string",
            "example": "22-07-2021"
          }
        }
      },
      "Applications": {
        "type": "object",
        "required": [
          "name",
          "opisanie"
        ],
        "properties": {
          "id": {
            "type": "integer",
            "example": "1"
          },
          "id_user": {
            "type": "integer",
            "example": "1"
          },
          "id_notification": {
            "type": "integer",
            "example": "1"
          },
          "files": {
            "type": "object"
          },
          "status": {
            "type": "string",
            "example": "На рассмотрении"
          }
        }
      },
      "Error": {
        "type": "object",
        "required": [
          "code",
          "message"
        ],
        "properties": {
          "code": {
            "type": "integer"
          },
          "message": {
            "type": "string"
          }
        }
