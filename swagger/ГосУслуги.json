      "Services": {
        "type": "object",
        "required": [
          "name",
          "opisanie"
        ],
        "properties": {
          "id": {
            "type": "integer",
            "example": "1"
          },
          "name": {
            "type": "string",
            "example": "Ипотека"
          },
          "opisanie": {
            "type": "string",
            "example": "Ипотека на квартиру на 342 года"
          },
          "document": {
            "type": "object"
          }
        }
      },
      "Auxiliary_table": {
        "type": "object",
        "required": [
          "id_application_user",
          "id_application"
